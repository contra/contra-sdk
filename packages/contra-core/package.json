{
  "name": "@contra/contra-core",
  "version": "0.2.0",
  "description": "Typed fetch wrapper and core API client for Contra",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"No tests yet\"",
    "lint": "echo \"No linting configured yet\""
  },
  "keywords": ["contra", "api", "client", "typescript", "fetch"],
  "author": "Contra",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/javron/contra-sdk.git",
    "directory": "packages/contra-core"
  },
  "homepage": "https://github.com/javron/contra-sdk#readme",
  "bugs": {
    "url": "https://github.com/javron/contra-sdk/issues"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
} 